{
  "expo": {
    "name": "boilerplate-react-native-mvc-application",
    "slug": "boilerplate-react-native-mvc-application",
    "version": "1.0.0",
    "package": "com.application.mvc.native.react.boilerplate",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vmcapplication",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.application.mvc.native.react.boilerplate",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "androidStatusBar": {
      "backgroundColor": "#00000000",
      "translucent": "true",
      "barStyle": "light-content"
    },
    "androidNavigationBar": {
      "backgroundColor": "#FFFFFF"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
